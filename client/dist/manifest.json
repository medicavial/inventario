{
  "name": "Inventario MV",
  "description": "Sistema de Inventario MV",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
        "storage",
        "unlimitedStorage",
        "alarms",
        "notifications",
        "http://api.medicavial.mx/api/",
        "https://fonts.googleapis.com/",
        "webview",
        "<all_urls>",
        "fullscreen"
    ],
  "icons": { "16": "favicon-16.png", "128": "favicon-120.png" },
  "content_security_policy": "script-src 'self'; object-src 'self'"
}

